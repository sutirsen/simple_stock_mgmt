<% provide(:title, "Add Coupon") %>
<div class="col-md-10">
  <div class="content-box-large">
    <div class="panel-heading">
          <div class="panel-title">Add Coupon</div>

          <div class="panel-options">
            <%= link_to coupons_path do %>
              <i class="glyphicon glyphicon-list" title="See all coupons"></i>
            <% end %>
          </div>
      </div>
    <div class="panel-body">
      <%= render 'form', coupon: @coupon %>
    </div>
  </div>
</div>
